#include<iostream>
using namespace std;

class student{
private:
int roll;
string name;

public:
void set_roll(int r){
	roll= r;
}	
void set_name(string n){
	name= n;
}
int get_roll(){
	return roll;
}
string get_name(){
	return name;
}
void add(student a){
cout<<a;
}
	
};	
	
int main(){
int r;
string n;

cout<<"Enter the roll number here: ";
cin>>r;
cout<<endl;
cout<<"Enter the name here: ";
cin>>n;
	
student record;
record.set_roll(r);
record.set_name(n);	



// This is second obj
student record2;


record2.add(record);

cout<<record.get_roll()<<endl;
cout<<record.get_name()<<endl;
cout<<"This is second obj"<<endl;
cout<<record2<<endl;



}	
	
